文法

    0 $accept: statements $end

    1 statements: statement NEWLINE
    2           | statements statement NEWLINE

    3 statement: create_table_statement
    4          | drop_table_statement
    5          | select_statement
    6          | insert_statement
    7          | delete_statement

    8 create_table_statement: CREATE TABLE NAME OPAREN attribute_type_list EPAREN

    9 attribute_type_list: NAME data_type
   10                    | NAME data_type COLON attribute_type_list

   11 data_type: INTTOK
   12          | STR20TOK

   13 drop_table_statement: DROP TABLE NAME

   14 select_statement: SELECT select_list FROM table_list
   15                 | SELECT DISTINCT select_list FROM table_list
   16                 | SELECT select_list FROM table_list WHERE search_condition
   17                 | SELECT DISTINCT select_list FROM table_list WHERE search_condition
   18                 | SELECT select_list FROM table_list ORDERBY column_name
   19                 | SELECT DISTINCT select_list FROM table_list ORDERBY column_name
   20                 | SELECT select_list FROM table_list WHERE search_condition ORDERBY column_name
   21                 | SELECT DISTINCT select_list FROM table_list WHERE search_condition ORDERBY column_name

   22 select_list: STAR_MULTIPLICATION
   23            | select_sublist

   24 select_sublist: column_name
   25               | column_name COLON select_sublist

   26 table_list: NAME
   27           | NAME COLON table_list

   28 insert_statement: INSERTINTO NAME OPAREN attribute_list EPAREN insert_tuples

   29 attribute_list: NAME
   30               | NAME COLON attribute_list

   31 insert_tuples: VALUES OPAREN value_list EPAREN
   32              | select_statement

   33 value_list: value
   34           | value COLON value_list

   35 value: LITERAL
   36      | INTEGER
   37      | TOKNULL

   38 delete_statement: DELETE FROM NAME
   39                 | DELETE FROM NAME WHERE search_condition

   40 search_condition: boolean_term
   41                 | boolean_term OR search_condition

   42 boolean_term: boolean_factor
   43             | boolean_factor AND boolean_term

   44 boolean_factor: boolean_primary
   45               | NOT boolean_primary

   46 boolean_primary: comparison_predicate
   47                | OPAREN search_condition EPAREN

   48 comparison_predicate: expression COMPOP expression

   49 expression: term
   50           | term PLUS expression
   51           | term MINUS expression

   52 term: factor
   53     | factor STAR_MULTIPLICATION term
   54     | factor DIVISION term

   55 factor: column_name
   56       | LITERAL
   57       | INTEGER
   58       | OPAREN expression EPAREN

   59 column_name: NAME
   60            | NAME DOT NAME


